{
  "name": "@zkpip/core",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": { "import": "./dist/index.js", "types": "./dist/index.d.ts" },
    "./kind": { "import": "./dist/kind.js", "types": "./dist/kind.d.ts" },
    "./keys/keyId": { "import": "./dist/keys/keyId.js", "types": "./dist/keys/keyId.d.ts" },
    "./seal/v1": { "import": "./dist/seal/v1.js", "types": "./dist/seal/v1.d.ts" },
    "./json": { "import": "./dist/json/index.js", "types": "./dist/json/index.d.ts" },
    "./json/*": { "import": "./dist/json/*.js", "types": "./dist/json/*.d.ts" },
    "./schemas/*": { "import": "./dist/schemas/*", "types": "./dist/schemas/*" }
  },
  "typesVersions": {
    "*": {
      "kind": ["dist/kind.d.ts"],
      "keys/keyId": ["dist/keys/keyId.d.ts"],
      "seal/v1": ["dist/seal/v1.d.ts"],
      "json": ["dist/json/index.d.ts"],
      "json/*": ["dist/json/*.d.ts"],
      "schemas/*": ["dist/schemas/*"],
      "*": ["dist/index.d.ts"]
    }
  },
  "version": "0.1.0",
  "private": false,
  "imports": {
    "#fs-compat": {
      "types": "./src/utils/fs-compat.ts",
      "default": "./dist/utils/fs-compat.js"
    }
  },
  "files": [
    "dist",
    "schemas",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=20.10"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -b -w tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "node ../../node_modules/eslint/bin/eslint.js -c ../../eslint.config.mjs \"src/**/*.ts\"",
    "lint:fix": "node ../../node_modules/eslint/bin/eslint.js -c ../../eslint.config.mjs \"src/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1"
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.15",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.36.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "tsx": "^4.20.5",
    "vitest": "^3.2.4"
  }
}
