{
  "name": "@zkpip/cli",
  "version": "0.1.0",
  "type": "module",
  "private": false,
  "bin": {
    "zkpip": "./dist/index.js"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=20.10"
  },
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "tsc -b && node ./scripts/make-executable.mjs",
    "build:watch": "tsc -b -w",
    "test": "vitest run",
    "lint": "eslint .",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "format": "prettier -w .",
    "format:check": "prettier -c ."
  },
  "dependencies": {
    "@types/yargs": "^17.0.33",
    "snarkjs": "^0.7.5",
    "yargs": "^18.0.0"
  }
}
