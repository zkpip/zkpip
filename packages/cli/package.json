  },
  "dependencies": {
    "@zkpip/core": "^0.1.0",
    "commander": "^14.0.0",
    "snarkjs": "^0.7.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "@types/yargs": "^17.0.33",
    "tsx": "^4.20.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0"    
  }
}
