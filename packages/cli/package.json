{
  "name": "@zkpip/cli",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "zkpip": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx src/index.ts",
